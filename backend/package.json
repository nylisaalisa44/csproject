  "description": "Backend for CS2 trading platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev:test": "nodemon src/index-test.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed:items": "ts-node src/scripts/seedItems.ts",
    "seed:bots": "node scripts/add-test-bots.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e"
  },
  "keywords": [
    "cs2",
    "trading",
    "steam",
    "skins"
  ],
  "author": "",
  "license": "ISC",
